    "preview": "node dist/src/server.js",
    "test": "jest --passWithNoTests --silent",
    "test:w": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "unit": "yarn test -c jest.config.unit.cjs",
    "unit:w": "yarn unit --watch",
    "unit:cov": "yarn unit --coverage",
    "int": "yarn test -c jest.config.integration.cjs",
